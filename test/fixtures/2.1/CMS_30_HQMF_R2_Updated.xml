	<!-- Measure Data Criteria Section -->
	<component>
		<dataCriteriaSection>
			<code code="57025-9" codeSystem="2.16.840.1.113883.6.1">
				<displayName value="Data criteria (QDM Data Elements)"/>
			</code>
			<title value="Data Criteria Section"/>
			<text>

				<xml>
					<list>
						<item>Patient age is greater or equal to 18 years prior to inpatient
							encounter</item>
						<item>Inpatient encounter's length of stay is less than or equal to 120
							days</item>
						<item>Inpatient encounter with required admission datetime</item>
						<item>Principal active diagnosis - acute myocardial infarction (AMI) during
							inpatient encounter</item>
						<item>Patient characteristic clinical trial participant during inpatient
							encounter</item>
						<item>Lab test result LDL-c less than 100mg/dL</item>
						<item>Lab test result LDL-c less than 100mg/dL within 24 hours after
							inpatient encounter admission datetime</item>
						<item>Lab test result LDL-c lt 100mg/dL within 30 days prior to inpatient
							encounter admission datetime</item>
						<item>Comfort Measure Only Intervention performed after inpatient encounter
							admission datetime</item>
						<item>Encounter discharge status: patient expired</item>
						<item>Encounter discharge status: discharge to another hospital</item>
						<item>Encounter discharge status: home hospice care</item>
						<item>Encounter discharge status: inpatient hospice care</item>
						<item>Encounter discharge status: left against medical advice</item>
						<item>Medication order: statins during inpatient encounter discharge
							datetime</item>
						<item>Active discharge medication: statins during inpatient encounter</item>
						<item>Not done: medication order for statins during inpatient encounter
							discharge datetime</item>
						<item>Not done: active discharge medication: statins during inpatient
							encounter</item>
						<item>Medication order statins not done during inpatient encounter with
							medical reason</item>
						<item>Medication order statin not done during inpatient encounter with
							patient reason</item>
						<item>Medication order statin not done during inpatient encounter with
							system reason</item>
					</list>
				</xml>
			</text>

			<!-- These definitions provide a model of specific criteria subtypes and should be referenced in the criterion. These particular definitions are
			provided for illustration purposes only, as they are optional. -->

			<definition>
				<observationDefinition>
					<id extension="Demographics" root="2.16.840.1.113883.3.1619.5148.1"/>
				</observationDefinition>
			</definition>
			<definition>
				<encounterDefinition>
					<id extension="Encounters" root="2.16.840.1.113883.3.1619.5148.1"/>
				</encounterDefinition>
			</definition>
			<definition>
				<observationDefinition>
					<id extension="Problems" root="2.16.840.1.113883.3.1619.5148.1"/>
				</observationDefinition>
			</definition>
			<definition>
				<observationDefinition>
					<id extension="Allergies" root="2.16.840.1.113883.3.1619.5148.1"/>
				</observationDefinition>
			</definition>
			<definition>
				<procedureDefinition>
					<id extension="Procedures" root="2.16.840.1.113883.3.1619.5148.1"/>
				</procedureDefinition>
			</definition>
			<definition>
				<observationDefinition>
					<id extension="Results" root="2.16.840.1.113883.3.1619.5148.1"/>
				</observationDefinition>
			</definition>
			<definition>
				<observationDefinition>
					<id extension="Vitals" root="2.16.840.1.113883.3.1619.5148.1"/>
				</observationDefinition>
			</definition>
			<definition>
				<substanceAdministrationDefinition>
					<id extension="Medications" root="2.16.840.1.113883.3.1619.5148.1"/>
				</substanceAdministrationDefinition>
			</definition>
			<definition>
				<supplyDefinition>
					<id extension="RX" root="2.16.840.1.113883.3.1619.5148.1"/>
				</supplyDefinition>
			</definition>

			<!-- Initial patient population: AMI patients >=18 years of age and length of stay <= 120days -->
			<!-- Principal active diagnosis - acute myocardial infarction (AMI) -->
			<entry typeCode="DRIV">
				<localVariableName value="principalActiveDiagnosis_AMI"/>
				<observationCriteria classCode="OBS" moodCode="EVN">
					<id root="e656021e-89a9-4c7c-a45d-74adf2378af0"
						extension="principalActiveDiagnosis_AMI"/>
					<code code="11450-4" codeSystem="2.16.840.1.113883.6.1">
						<displayName value="Problem"/>
					</code>
					<statusCode code="completed"/>
					<!-- principal diagnosis -->
					<priorityCode>
						<item valueSet="2.16.840.1.113883.3.117.2.7.1.14" >
							<displayName value="principal diagnosis"/>
						</item>
					</priorityCode>
					<!-- AMI diagnosis value set -->
					<value xsi:type="CD" valueSet="2.16.840.1.113883.3.666.5.3011">
						<displayName value="Hospital Measures - AMI Grouping Value Set"/>
					</value>
				</observationCriteria>
			</entry>

			<!-- Inpatient encounter <=120 days and an AMI diagnosis  -->
			<entry typeCode="DRIV">
				<localVariableName value="inpatientEncounterAMI"/>
				<encounterCriteria classCode="ENC" moodCode="EVN">
					<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
						extension="inpatientEncounterAMI"/>
					<code valueSet="2.16.840.1.113883.3.666.5.3001">
						<displayName value="Encounter Inpatient SNOMED-CT Value Set"/>
					</code>
					<title value="Performed inpatient encounter"/>
					<statusCode code="completed"/>
					<!-- Inpatient encounter's length of stay is less than or equal to 120 days -->
					<lengthOfStayQuantity>
						<uncertainRange lowClosed="true" highClosed="true">
							<low xsi:type="PQ" value="0" unit="d"/>
							<high xsi:type="PQ" value="120" unit="d"/>
						</uncertainRange>
					</lengthOfStayQuantity>
					<temporallyRelatedInformation typeCode="EDU">
						<criteriaReference classCode="OBS" moodCode="EVN">
							<id root="2.16.840.1.113883.3.100.100.123" extension="MeasurePeriod"/>
						</criteriaReference>
					</temporallyRelatedInformation>
					<!-- Principal active diagosis is AMI -->
					<temporallyRelatedInformation typeCode="OVERLAP">
						<criteriaReference classCode="OBS" moodCode="EVN">
							<!-- referencing the inpatient encounter via id -->
							<id root="e656021e-89a9-4c7c-a45d-74adf2378af0"
								extension="principalActiveDiagnosis_AMI"/>
						</criteriaReference>
					</temporallyRelatedInformation>
				</encounterCriteria>
			</entry>

			<!-- Patient age is greater or equal to 18 years prior to inpatient encounter  -->
			<entry typeCode="DRIV">
				<localVariableName value="ageGreaterThanOrEqualTo18Y"/>
				<observationCriteria moodCode="EVN" classCode="OBS">
					<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
						extension="PatientCharacteristicBirthdate"/>
					<code codeSystemName="LOINC" codeSystem="2.16.840.1.113883.6.1" code="21112-8">
						<displayName value="birth date"/>
					</code>
					<text value="Patient Characteristic Birthdate: birth date"/>
					<temporallyRelatedInformation typeCode="SCW">
						<pauseQuantity>
							<uncertainRange lowClosed="true">
								<low xsi:type="PQ" value="18" unit="a"/>
								<high xsi:type="PQ" nullFlavor="PINF"/>
							</uncertainRange>
						</pauseQuantity>
						<criteriaReference classCode="ENC" moodCode="EVN">
							<!-- referencing the inpatient encounter via id -->
							<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
								extension="inpatientEncounterAMI"/>
						</criteriaReference>
					</temporallyRelatedInformation>
				</observationCriteria>
			</entry>

			<!-- Numerator: statin during valid encounter or prescribed at discharge -->
			<!-- Medication order: statins during inpatient encounter discharge datetime -->
			<entry typeCode="DRIV">
				<localVariableName value="statinMedOrderOnDischarge"/>
				<!-- moodeCode="RQO": medication order request -->
				<substanceAdministrationCriteria classCode="SBADM" moodCode="INT">
					<id root="550f0dd0-cc77-11e1-9b23-0800200c9a66"
						extension="statinMedOrderOnDischarge"/>
					<!-- optional title -->
					<title value="Statin medication order at encounter discharge"/>
					<!-- drug: statins -->
					<participation typeCode="CSM">
						<role classCode="MANU">
							<playingMaterial classCode="MMAT" determinerCode="KIND">
								<code code="2.16.840.1.113883.3.117.1.7.1.824"
									codeSystem="2.16.840.1.113883.3.560.101.1">
									<displayName value="Statins RxNorm Value Set"/>
								</code>
							</playingMaterial>
						</role>
					</participation>
					<temporallyRelatedInformation typeCode="SDU">
						<criteriaReference classCode="ENC" moodCode="EVN">
							<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
								extension="inpatientEncounterAMI"/>
						</criteriaReference>
					</temporallyRelatedInformation>
				</substanceAdministrationCriteria>
			</entry>

			<!-- Denonminator Exclusions: comfort measures only; enrolled in clinical trials;
				 discharged to another hospital, against medical advice, due to death, to hospice care;
				  -->
			<!-- Patient characteristic clinical trial participant XXXduring inpatient encounterXXX -->
			<entry typeCode="DRIV">
				<localVariableName value="clinicalTrialParticipant"/>
				<observationCriteria classCode="OBS" moodCode="EVN">
					<id root="b1866385-d91b-49d2-a12d-13b6334f67a3"
						extension="clinicalTrialParticipant"/>
					<code code="428024001" codeSystem="2.16.840.1.113883.6.96">
						<displayName value="Clinical Trial Participant"/>
					</code>
					<value xsi:type="CD" code="2.16.840.1.113883.3.526.2.643"
						codeSystem="2.16.840.1.113883.3.560.101.1">
						<displayName value="Clinical Trial Participant SNOMED-CT Value Set"/>
					</value>
					<temporallyRelatedInformation typeCode="OVERLAP">
						<criteriaReference classCode="ENC" moodCode="EVN">
							<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
								extension="inpatientEncounterAMI"/>
						</criteriaReference>
					</temporallyRelatedInformation>
				</observationCriteria>
			</entry>

			<!-- Comfort Measure Only Intervention XXXperformed after inpatient encounter admission datetimeXXX
			 . -->
			<entry typeCode="DRIV">
				<localVariableName value="InterventionPerformedComfortMeasureOnly"/>
				<procedureCriteria classCode="PROC" moodCode="EVN">
					<id root="7d559e0a-4e01-4f52-b87b-a3342705e2f2"
						extension="InterventionPerformedComfortMeasureOnly"/>
					<code code="2.16.840.1.113883.3.666.5.3002"
						codeSystem="2.16.840.1.113883.3.560.101.1">
						<displayName
							value="Hospital Measures - Comfort Measure Only Intervention SNOMED-CT Value Set"/>
					</code>
					<temporallyRelatedInformation typeCode="SDU">
						<criteriaReference classCode="ENC" moodCode="EVN">
							<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
								extension="inpatientEncounterAMI"/>
						</criteriaReference>
					</temporallyRelatedInformation>
				</procedureCriteria>
			</entry>

			<!-- Patient discharged against medical advice, to inpatient hospice care, to home hospice care, to another hospital during encounter, due to death -->
			<entry typeCode="DRIV">
				<encounterCriteria classCode="ENC" moodCode="EVN">
					<id root="e9a068dc-310a-48e4-95b0-eed852a8950b" extension="discharged"/>
					<!-- This valueSet is defined in a definition element above -->
					<dischargeDispositionCode valueSet="2.16.840.1.113883.3.666.5.1234567"
						codeSystem="2.16.840.1.113883.3.560.101.1">
						<displayName value="left against medical advice value set"/>
					</dischargeDispositionCode>
				</encounterCriteria>
			</entry>

			<!-- Intersection of: encounters where patient was discharged with above reasons, with, inpatient AMI encounters.
				Result: all encounters with AMI diagnosis AND patient was discharged with above reasons -->
			<entry typeCode="DRIV">
				<grouperCriteria classCode="GROUPER" moodCode="EVN">
					<id root="dd732e30-cc87-11e1-9b23-0800200c9a66"
						extension="dischargedDuringInpatientEncounterAMI"/>
					<!-- encounters with discharge reasons -->
					<outboundRelationship typeCode="COMP">
						<conjunctionCode code="AND"/>
						<criteriaReference classCode="ENC" moodCode="EVN">
							<id root="e9a068dc-310a-48e4-95b0-eed852a8950b" extension="discharged"/>
						</criteriaReference>
					</outboundRelationship>
					<!-- intersect with Inpatient AMI Encounters -->
					<outboundRelationship typeCode="COMP">
						<conjunctionCode code="AND"/>
						<criteriaReference classCode="ENC" moodCode="EVN">
							<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
								extension="inpatientEncounterAMI"/>
						</criteriaReference>
					</outboundRelationship>
				</grouperCriteria>
			</entry>


			<!-- Denominator Exceptions: with LDL<100 between 30 days prior and 24 hours after admission and no statin given;
				 reason for no statin: medical reason, patient reason, system reason -->

			<!-- Lab test result LDL-c less than 100mg/dL 24 after inpatient encounter start-->
			<entry typeCode="DRIV">
				<localVariableName value="LDLResultLessThan100"/>
				<observationCriteria classCode="OBS" moodCode="EVN">
					<id root="a0d5b1b1-bf33-4a6d-b34d-3022b818da69"
						extension="LDLResultLessThan100_24hr"/>
					<!-- LDL lab test scoped by value set -->
					<code valueSet="2.16.840.1.113883.3.117.1.7.1.215"
						codeSystem="2.16.840.1.113883.3.560.101.1">
						<displayName value="LDL-c LOINC Value Set"/>
					</code>
					<statusCode code="completed"/>
					<!-- result is less than 100 mg/dL -->
					<value xsi:type="IVL_PQ" highClosed="false">
						<high value="100" unit="mg/dL"/>
					</value>
					<temporallyRelatedInformation typeCode="SCW">
						<pauseQuantity>
							<uncertainRange lowClosed="true" highClosed="true">
								<low xsi:type="PQ" value="-24" unit="h"/>
								<high xsi:type="PQ" value="0" unit="h"/>
							</uncertainRange>
						</pauseQuantity>
						<!-- LDL<100 within 24hours of inpatient encounter -->
						<criteriaReference classCode="ENC" moodCode="EVN">
							<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
								extension="inpatientEncounterAMI"/>
						</criteriaReference>
					</temporallyRelatedInformation>
				</observationCriteria>
			</entry>

			<!-- LDL<100 up to 30 days prior to admit and no statins -->
			<entry typeCode="DRIV">
				<localVariableName value="LDLlt100_30days"/>
				<observationCriteria classCode="OBS" moodCode="EVN">
					<id root="a0d5b1b1-bf33-4a6d-b34d-3022b818da69"
						extension="LDLResultLessThan100_30d"/>
					<!-- LDL lab test scoped by value set -->
					<code valueSet="2.16.840.1.113883.3.117.1.7.1.215"
						codeSystem="2.16.840.1.113883.3.560.101.1">
						<displayName value="LDL-c LOINC Value Set"/>
					</code>
					<statusCode code="completed"/>
					<!-- result is less than 100 mg/dL -->
					<value xsi:type="IVL_PQ" highClosed="false">
						<high value="100" unit="mg/dL"/>
					</value>
					<temporallyRelatedInformation typeCode="SCW">
						<pauseQuantity>
							<uncertainRange lowClosed="true" highClosed="true">
								<low xsi:type="PQ" value="0" unit="d"/>
								<high xsi:type="PQ" value="30" unit="d"/>
							</uncertainRange>
						</pauseQuantity>
						<!-- LDL<100 within 30 days before inpatient encounter -->
						<criteriaReference classCode="ENC" moodCode="EVN">
							<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
								extension="inpatientEncounterAMI"/>
						</criteriaReference>
					</temporallyRelatedInformation>
				</observationCriteria>
			</entry>

			<!-- Patient NOT prescribed statin (both types) actionNegationInd=true DURING inpatient encounter-->
			<entry typeCode="DRIV">
				<!-- moodeCode="RQO": medication order request -->
				<substanceAdministrationCriteria classCode="SBADM" moodCode="RQO"
					actionNegationInd="true">
					<id root="550f0dd0-cc77-11e1-9b23-0800200c9a66"
						extension="negStatinMedOrderOnDischarge"/>
					<!-- optional title -->
					<title value="Statin medication order at encounter discharge"/>
					<!-- drug: statins -->
					<participation typeCode="CSM">
						<role classCode="MANU">
							<playingMaterial classCode="MMAT" determinerCode="KIND">
								<code code="2.16.840.1.113883.3.117.1.7.1.824"
									codeSystem="2.16.840.1.113883.3.560.101.1">
									<displayName value="Statins RxNorm Value Set"/>
								</code>
							</playingMaterial>
						</role>
					</participation>
					<temporallyRelatedInformation typeCode="DURING">
						<criteriaReference classCode="ENC" moodCode="EVN">
							<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
								extension="inpatientEncounterAMI"/>
						</criteriaReference>
					</temporallyRelatedInformation>
				</substanceAdministrationCriteria>
			</entry>

			<!-- patient not on statin medication DURING inpatient encounter-->
			<entry typeCode="DRIV">
				<substanceAdministrationCriteria classCode="SBADM" moodCode="EVN"
					actionNegationInd="true">
					<id root="0bd01c10-cc7a-11e1-9b23-0800200c9a66"
						extension="negActiveDischargeMedStatin"/>
					<!-- the type of medication is discharge medication -->
					<code valueSet="2.16.840.1.113883.3.526.2.1075"
						codeSystem="2.16.840.1.113883.3.560.101.1">
						<displayName value="Discharge Medication"/>
					</code>
					<!-- active status -->
					<statusCode code="active"/>
					<participation typeCode="CSM">
						<role classCode="MANU">
							<playingMaterial classCode="MMAT" determinerCode="KIND">
								<code code="2.16.840.1.113883.3.117.1.7.1.824"
									codeSystem="2.16.840.1.113883.3.560.101.1">
									<displayName value="Statins RxNorm Value Set"/>
								</code>
							</playingMaterial>
						</role>
					</participation>
					<temporallyRelatedInformation typeCode="DURING">
						<criteriaReference classCode="ENC" moodCode="EVN">
							<id root="e9a068dc-310a-48e4-95b0-eed852a8950b"
								extension="inpatientEncounterAMI"/>
						</criteriaReference>
					</temporallyRelatedInformation>
				</substanceAdministrationCriteria>
			</entry>

			<!-- Reasons for no Statins: Medical Reasons, Patient Reasons, System Reasons -->
			<entry typeCode="DRIV">
				<observationCriteria classCode="OBS" moodCode="EVN">
					<id root="ee732e30-cc87-11e1-9b23-0800200c9a77" extension="Reasons"/>
					<code code="ASSERTION" codeSystem="2.16.840.1.113883.5.4"/>
					<value xsi:type="CD" valueSet="2.16.840.1.113883.3.666.5.123456"
						codeSystem="2.16.840.1.113883.3.560.101.1"> </value>
				</observationCriteria>
			</entry>

			<!-- Statins not done during inpatient encounter with medical reason, patient reason, or system reason -->
