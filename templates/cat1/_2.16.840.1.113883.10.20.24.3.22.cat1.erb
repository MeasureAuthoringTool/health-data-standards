<entry>
  <encounter classCode="ENC" moodCode="RQO">
    <!--  Plan of Care Activity Encounter template -->
    <templateId root="2.16.840.1.113883.10.20.22.4.40" <% if !r2_compatibility %>extension="2014-06-09"<%end%>/>
    <!-- Encounter order template -->
    <templateId root="2.16.840.1.113883.10.20.24.3.22" <% if !r2_compatibility %>extension="2014-12-01"<%end%>/>
    <id root="1.3.6.1.4.1.115" extension="<%= entry.id %>"/>
    <%== code_display(entry, 'preferred_code_sets' => ['SNOMED-CT', 'ICD-9-CM', 'ICD-10-CM', 'CPT', 'HCPCS', 'CDT'], 'value_set_map' => filtered_vs_map, 'extra_content' => "sdtc:valueSet=\"#{value_set_oid}\"") %>
    <text><%= entry.description %></text>
    <statusCode code="<%= r2_compatibility ? 'new' : 'active' %>"/>
  
    <!-- Attribute: datetime -->
    <author>
      <templateId root="2.16.840.1.113883.10.20.22.4.119"/>
      <time <%= value_or_null_flavor(entry.start_time) %>/>
      <assignedAuthor>
        <id root="<%= UUID.generate %>"/>
      </assignedAuthor>
    </author>

  </encounter>
</entry>